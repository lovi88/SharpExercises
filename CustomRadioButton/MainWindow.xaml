        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CustomRadioButton"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>

        <local:IconRadioButton 
            SourceOff="/Icons/font-awesome_4-7-0_thumbs-down_256_0_007dff_none.png" 
            SourceOn="/Icons/font-awesome_4-7-0_thumbs-up_256_0_007dff_none.png" Text="SuperText" RadioGroupName="SuperGroup"/>

        <local:IconRadioButton SourceOff="/Icons/font-awesome_4-7-0_toggle-right_256_0_007dff_none.png" 
